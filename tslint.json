{
	"defaultSeverity": "error",
	"extends": ["tslint:recommended"],
	"jsRules": {},
	"rules": {
		"typedef": [
			true,
			"call-signature",
			"arrow-call-signature",
			"parameter",
			"arrow-parameter",
			"property-declaration",
			"member-variable-declaration"
		],
		"max-line-length": [
			true,
			{
				"limit": 120,
				"ignore-pattern": "^import |^export {(.*?)}|//|^\\s*it\\('|^\\s*describe\\('"
			}
		],
		"indent": false,
		"no-console": false,
		"no-string-literal": true,
		"quotemark": [true, "single", "avoid-escape", "avoid-template"],
		"array-type": [true, "array"],
		"object-literal-sort-keys": false,
		"prefer-for-of": false,
		"whitespace": [
			true,
			"check-branch",
			"check-decl",
			"check-operator",
			"check-separator",
			"check-type"
		]
	},
	"rulesDirectory": []
}
